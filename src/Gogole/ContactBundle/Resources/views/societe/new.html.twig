{% extends 'GogoleMainBundle:Main:index.html.twig' %}

{% block body %}

    <h2 class="text-center">Créer une société</h2>
    <div class="row">
    	<div class="col-md-offset-5">
			{{ form_start(form) }}
				<p>{{ form_row(form.nomSociete) }}</p>
				<p>{{ form_row(form.siretSociete) }}</p>
				<p>{{ form_row(form.eMailSociete) }}</p>
				<p>{{ form_row(form.telSociete) }}</p>	
				<input type="submit" name="envoyer" class="btn btn-default" value="Créer" />
				
			{{ form_end(form) }}
		</div>
	</div>
{% endblock %}
